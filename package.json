{
  "name": "@syedgalib/vsce-php-getter-and-setter",
  "displayName": "PHP Getter And Setter",
  "publisher": "SyedGalibAhmed",
  "description": "This extension generates getter and setter for given variable",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Snippets", "Keymaps"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "php",
        "path": "./snippets/snippets.code-snippets"
      }
    ],
    "keybindings": [
      {
        "key": "alt+cmd+,",
        "command": "editor.action.insertSnippet",
        "args": { "name": "generate_getter_and_setter_with_type_cast" }
      },
      {
        "key": "shift+alt+cmd+,",
        "command": "editor.action.insertSnippet",
        "args": { "name": "generate_getter_and_setter_without_type_cast" }
      }
    ]
  },
  "repository": {
    "url": "https://github.com/syedgalib/vs-code-extension-php-getter-and-setter"
  }
}
